let arr = [[1,2],[3,4],[5,6],[7,8]]

using inbuilt flat
arr.flat(infinity)
................................
function flat(){
  let arr1 = []
  arr.forEach((item)=>{
    arr1.push(...item)
  })
  return arr1
}
console.log(flat(arr))
.................................
using reduce
let result = arr.reduce((acc, curr) => acc.concat(curr), []);
console.log(result);
